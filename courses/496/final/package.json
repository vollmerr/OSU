{
  "name": "vollmerr-cs496-final",
  "version": "1.0.0",
  "engines": {
    "node": ">=4.3.2"
  },
  "description": "Final project for cs 496 - Node js API",
  "repository": "",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "start:dev": "nodemon npm start",
    "lint": "eslint --ignore-path .gitignore",
    "lint:js": "npm run lint -- . ",
    "format": "pretty-quick .",
    "format:staged": "pretty-quick --staged",
    "precommit": "cross-env lint-staged && npm run format:staged"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"]
  },
  "author": "Ryan Vollmer",
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": ["app.js"],
        "msg": "Boat + Slip tests..."
      }
    }
  },
  "license": "Apache-2.0",
  "semistandard": {
    "globals": ["after", "afterEach", "before", "beforeEach", "describe", "it"]
  },
  "dependencies": {
    "@google-cloud/datastore": "1.3.4",
    "body-parser": "1.18.2",
    "delay": "2.0.0",
    "express": "4.16.2",
    "joi": "13.2.0",
    "lodash": "4.17.5",
    "morgan": "1.9.0",
    "nconf": "0.10.0",
    "nodemon": "1.17.3",
    "prompt": "1.0.0",
    "uglify-js": "3.3.12",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "cross-env": "5.1.6",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "2.6.0",
    "husky": "0.14.3",
    "lint-staged": "7.1.2",
    "prettier": "1.12.1",
    "pretty-quick": "1.6.0"
  }
}
